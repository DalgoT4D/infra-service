# AWS Credentials
aws_access_key = "YOUR_AWS_ACCESS_KEY"
aws_secret_key = "YOUR_AWS_SECRET_KEY"

# RDS Instance Configuration
rdsname = "your-existing-rds-instance"  
RDS_DOMAIN = "your-rds-instance.region.rds.amazonaws.com"

# PostgreSQL Configuration
POSTGRES_USER = "admin"          
POSTGRES_PASSWORD = "your_password_here"  
APP_DB_NAME = "new_database"      
APP_DB_USER = "new_db_user"       
APP_DB_PASS = "new_user_password"   

# Optional: If you are using a custom database port (typically 5432 for PostgreSQL)
DB_PORT = 5432  # Default PostgreSQL port
